@model TestWebApp.Data.Fibonnacci
@{
    ViewBag.Title = "Fibonnacci Sequence";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <h1>Fibonnacci Sequence</h1>
    <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th>Position #</th>
                <th>Fibonnacci Number</th>
            </tr>
        </thead>
        @for (int i = 0; i < Model.fibSequence.Count; i++)
        {
            <tr>
                <td>@(i+1)</td>
                <td>@Model.fibSequence[i]</td>
            </tr>

        }
    </table>
    </div>
